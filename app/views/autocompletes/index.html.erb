<p id="notice"><%= notice %></p>

<h1>Autocompletes</h1>

<table>
  <thead>
    <tr>
      <th>Line 1</th>
      <th>Line 2</th>
      <th>Suburb</th>
      <th>City</th>
      <th>Province</th>
      <th>Postal code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @autocompletes.each do |autocomplete| %>
      <tr>
        <td><%= autocomplete.line_1 %></td>
        <td><%= autocomplete.line_2 %></td>
        <td><%= autocomplete.suburb %></td>
        <td><%= autocomplete.city %></td>
        <td><%= autocomplete.province %></td>
        <td><%= autocomplete.postal_code %></td>
        <td><%= link_to 'Show', autocomplete %></td>
        <td><%= link_to 'Edit', edit_autocomplete_path(autocomplete) %></td>
        <td><%= link_to 'Destroy', autocomplete, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Autocomplete', new_autocomplete_path %>
